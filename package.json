{
  "name": "api",
  "version": "0.16.0",
  "description": "List of lambda functions to serve as the API for the Global Train API.",
  "scripts": {
    "test": "mocha tests/",
    "hello": "sls invoke -f test",
    "deploy": "sh ./scripts/deploy.sh",
    "hello:local": "sls invoke local -f test",
    "register:local": "sls invoke local -f register -p ./dummy-data/register.json",
    "login:local": "sls invoke local -f login -p ./dummy-data/login.json",
    "login": "sls invoke -f login -p ./dummy-data/login.json"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.25.3",
    "mongoose": "^5.9.13"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2"
  },
  "keywords": [
    "lambda"
  ],
  "author": "jaak",
  "license": "ISC"
}
